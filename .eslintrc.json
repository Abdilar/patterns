{
  "env": {
      "browser": true,
      "es2024": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "globals": {
    "globalThis": "readable"
  },
  "rules": {
    "curly": "error",
    "semi": "error",
    "quotes": ["error", "single"]
  },
  "noInlineConfig": false,
  "reportUnusedDisableDirectives": true
}