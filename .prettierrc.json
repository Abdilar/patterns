{
	"tabWidth": 2,
	"useTabs": false,
	"printWidth": 90,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"quoteProps": "consistent",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"trailingComma": "none",
	"experimentalTernaries": true,
	"requirePragma": false,
	"insertPragma": false,
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto",
	"singleAttributePerLine": true
}
